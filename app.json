{
  "expo": {
    "name": "Tizly",
    "slug": "Tizly",
    "owner": "tizly",
    "version": "1.4",
    "orientation": "portrait",
    "icon": "./assets/appIcons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/appIcons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to photos to upload photos from your library."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.tizly.Tizly",
      "buildNumber": "9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Tizly wants to access your camera roll"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9f796f6e-9bef-444e-a342-2b5f10529e1b"
      }
    }
  }
}
